{
	"name": "Speedstack",
	"description": "Speedstack chrome packaged app",
	"version": "0.1",
	"app": {
		"background": {
			"scripts": ["public/js/chrome-app.js"]
		}
	},
	"permissions": [
		"<all_urls>",
		"notifications",
		{ 
			"mediaGalleries": ["read", "delete", "copyTo" , "allAutoDetected"]
		},
		"storage"
	]
}